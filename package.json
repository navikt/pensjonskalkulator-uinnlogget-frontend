{
  "name": "pensjonskalkulator-uinnlogget-frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build && npm run build:server",
    "build:prod": "cp .env.prod .env && next build && npm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "next start",
    "dev": "concurrently \"next dev --turbopack\" \"json-server mockdb/db.json --port 3001 \"",
    "fetch-dev-types": "openapi-typescript https://pensjonskalkulator-backend.intern.dev.nav.no/v3/api-docs/current -o ./src/types/schema.d.ts",
    "format:types": "prettier --write ./src/types/schema.d.ts && eslint ./src/types/schema.d.ts --fix",
    "test": "jest",
    "testc": "jest --c",
    "typecheck": "tsc --noEmit --pretty -p tsconfig.json && tsc --noEmit --pretty -p tsconfig.server.json",
    "lint": "next lint",
    "prettier:fix": "prettier --write '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@navikt/ds-css": "^7.23.1",
    "@navikt/ds-react": "^7.23.1",
    "@navikt/nav-dekoratoren-moduler": "^3.2.3",
    "@navikt/oasis": "^3.8.0",
    "@navikt/steng-for-regulering": "^0.0.7",
    "express": "^5.1.0",
    "highcharts": "^12.2.0",
    "highcharts-react-official": "^3.2.2",
    "immer": "^10.1.1",
    "next": "15.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.15.29",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "babel-jest": "^30.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^16.1.2",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.5.3",
    "typescript": "^5",
    "typescript-eslint": "^8.34.1"
  },
  "lint-staged": {
    "**/*.{cjs,js,jsx,ts,tsx}": "next lint --fix --file",
    "**/*.{cjs,js,jsx,ts,tsx,css}": "prettier --write"
  },
  "overrides": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
